{
    "name": "dvv/advent-of-code",
    "description": "Advent of Code",
    "license": "MIT",
    "authors": [
        {
            "name": "Dick van Viegen",
            "email": "dickv.viegen@gmail.com"
        }
    ],
    "require": {
        "php":"^8.1",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "codedungeon/phpunit-result-printer": "^0.31.0",
        "roave/security-advisories": "dev-latest",
        "symfony/dotenv": "^6.0"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/*/Test/*",
            "**/*/Tests/*",
            "**/*/test/*",
            "**/*/tests/*"
        ],
        "psr-4": {
            "marvellanius\\Advent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "marvellanius\\Advent\\": "test/unit/"
        }
    }
}
